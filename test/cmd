echo ">GS-linker" > GS.fasta
echo GGGGSGGGGSGGGGS >> GS.fasta
python ../tools/make_resfile.py -p A.pdb -r A.resfile
python ../tools/make_resfile.py -f GS.fasta -s 60 -r GS.resfile
python ../tools/make_resfile.py -l 60 -s 75
cat A.resfile > design.resfile
cat GS.resfile >> design.resfile
cat s75l60.resfile >> design.resfile
python ../tools/make_cstfile.py -p A.pdb -c design.cstfile --type dist
python ../tools/hallucinate.py -l 134 -r design.resfile -c design.cstfile --trmodel ../models/trmodel/ --background ../models/bkgrd/

